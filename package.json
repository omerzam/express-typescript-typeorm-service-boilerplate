{
   "name": "boilerplate-service",
   "version": "1.0.0",
   "description": "Express typescript typeorm boilerplate Service",
   "main": "app.js",
   "scripts": {
      "build": "tsc",
      "start": "node ./dist/server.js",
      "clean": "rm -rf ./dist && mkdir dist",
      "dev": "nodemon --config nodemon.json src/server.ts",
      "dev:debug": "nodemon --config nodemon.json --inspect-brk src/server.ts",
      "test": "jest",
      "test:watch": "jest --watchAll",
      "lint": "ts-standard",
      "lint:fix": "ts-standard --fix",
      "prepare": "husky install",
      "migrations:generate": "npx ts-node ./node_modules/.bin/typeorm migration:generate -f ./src/config/database.ts",
      "migrations:run": "npx ts-node ./node_modules/.bin/typeorm migration:run -f ./src/config/database.ts",
      "migrations:revert": "npx ts-node ./node_modules/.bin/typeorm migration:revert -f ./src/config/database.ts"
   },
   "keywords": [
      "express.js",
      "express",
      "typescript",
      "typeorm",
      "boilerplate",
      "microservice"
   ],
   "author": "Omer Z.",
   "license": "ISC",
   "dependencies": {
      "axios": "^0.21.4",
      "class-validator": "^0.13.1",
      "convict": "^5.2.1",
      "cors": "^2.8.5",
      "express": "^4.17.1",
      "express-request-id": "^1.4.1",
      "helmet": "^4.6.0",
      "lodash": "^4.17.21",
      "moment": "^2.29.1",
      "pg": "^8.7.1",
      "pino": "^6.13.3",
      "pino-http": "^5.8.0",
      "pino-pretty": "^5.1.3",
      "reflect-metadata": "^0.1.13",
      "regenerator-runtime": "^0.13.7",
      "response-time": "^2.3.2",
      "typeorm": "^0.2.38",
      "uuid": "^8.3.1"
   },
   "devDependencies": {
      "@tsconfig/node12": "^1.0.9",
      "@types/bunyan": "^1.8.7",
      "@types/convict": "^6.1.1",
      "@types/cors": "^2.8.12",
      "@types/express": "^4.17.13",
      "@types/express-request-id": "^1.4.3",
      "@types/jest": "^27.0.2",
      "@types/lodash": "^4.14.179",
      "@types/node": "^8.10.66",
      "@types/pino": "^6.3.11",
      "@types/pino-http": "^5.8.0",
      "@types/response-time": "^2.3.5",
      "@types/supertest": "^2.0.11",
      "husky": "^7.0.2",
      "jest": "^27.2.5",
      "nodemon": "^2.0.13",
      "supertest": "^6.1.6",
      "ts-jest": "^27.0.5",
      "ts-node": "^10.3.0",
      "ts-standard": "^10.0.0",
      "typescript": "^4.4.4"
   }
}
